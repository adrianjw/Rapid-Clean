        <servlet-class>com.uts.rapid.clean.controller.CleanerOrderRejectedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionServlet</servlet-name>
        <servlet-class>com.uts.rapid.clean.controller.SessionServlet</servlet-class>
