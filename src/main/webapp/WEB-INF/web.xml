        <servlet-class>com.uts.rapid.clean.controller.CleanerSignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderServlet</servlet-name>
        <servlet-class>com.uts.rapid.clean.controller.OrderServlet</servlet-class>
