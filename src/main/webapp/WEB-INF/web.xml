    <servlet>
        <servlet-name>OrderPayServlet</servlet-name>
        <servlet-class>com.uts.rapid.clean.controller.OrderPayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerSignUpServlet</servlet-name>
        <url-pattern>/CustomerSignUpServlet</url-pattern>
